{
    "mcpServers": {
        "devops-mcp": {
			"command": "docker",
			"args": [
				"run",
				"-i",
				"--rm",
				"--env-file",
				".env",
				"devopsmcp:local"
			],
			"type": "stdio"
		},
		"appinsights": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "--env-file",
                ".env",
                "appinsightsmcp:local"
            ],
            "type": "stdio"
        }
    }
}
